    <url-pattern>/app2/*</url-pattern>
    <url-pattern>/app3/*</url-pattern>
    <url-pattern>/app4/*</url-pattern>
    <url-pattern>/app5/*</url-pattern>
    <url-pattern>/app6/*</url-pattern>
  </filter-mapping>
</web-app>